set -g pane-active-border-style "fg=#268bd2"
set -g pane-border-style "fg=#93a1a1"
set -g status-left "#[bg=#fdf6e3]"
set -g status-right "#[bg=#fdf6e3] #[bg=#268bd2,fg=#fdf6e3,bold] ó°Œ¢ #h "
set -g status-style "bg=#fdf6e3,fg=#657b83"
setw -g mode-style "bg=#268bd2,fg=#fdf6e3,bold"
setw -g window-status-activity-style "bg=#fdf6e3,fg=#b58900"
